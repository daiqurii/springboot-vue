{"ast":null,"code":"import request from \"../utils/request\";\nexport default {\n  name: \"Home\",\n  components: {},\n  data() {\n    return {\n      url: \"http://localhost:9090\",\n      currentPage: 1,\n      pageSize: 10,\n      total: 0,\n      form: {},\n      dialogVisible: false,\n      search: \"\",\n      tableData: []\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    load() {\n      var paramsObj = {\n        pageNum: this.currentPage,\n        pageSize: this.pageSize,\n        search: this.search\n      };\n      request.get(this.url + \"/user/getAll\", {\n        params: paramsObj\n      }).then(res => {\n        this.tableData = res.obj.records;\n        this.total = res.obj.records.length;\n      });\n    },\n    add() {\n      // 清空表单 才能添加新对象\n      this.form = {};\n      //    打开弹窗 Dialog并添加信息\n      this.dialogVisible = true;\n    },\n    save() {\n      if (this.form.id) {\n        request.put(this.url + \"/user/put\", this.form).then(res => {\n          console.log(res);\n          this.load();\n        });\n      } else {\n        request.post(this.url + \"/user/add\", this.form).then(res => {\n          console.log(res);\n          this.load();\n        });\n      }\n      this.dialogVisible = false;\n    },\n    handleEdit(row) {\n      this.form = JSON.parse(JSON.stringify(row)); //深拷贝\n      this.dialogVisible = true;\n    },\n    handleClick() {},\n    handleSizeChange() {},\n    handleCurrentChange() {}\n  }\n};","map":{"version":3,"mappings":"AAyGI,OAAOA,OAAM,MAAO,kBAAkB;AAEtC,eAAe;EACXC,IAAI,EAAE,MAAM;EACZC,UAAU,EAAE,CAAC,CAAC;EACdC,IAAI,GAAG;IACH,OAAO;MACHC,GAAG,EAAC,uBAAuB;MAC3BC,WAAW,EAAC,CAAC;MACbC,QAAQ,EAAC,EAAE;MACXC,KAAK,EAAC,CAAC;MACPC,IAAI,EAAE,CAAC,CAAC;MACRC,aAAa,EAAE,KAAK;MACpBC,MAAM,EAAE,EAAE;MACVC,SAAS,EAAE;IACf,CAAC;EACL,CAAC;EACDC,OAAO,GAAG;IACN,IAAI,CAACC,IAAI,EAAC;EACd,CAAC;EACDC,OAAO,EAAE;IACLD,IAAI,GAAE;MACF,IAAIE,SAAQ,GAAI;QACZC,OAAO,EAAC,IAAI,CAACX,WAAW;QACxBC,QAAQ,EAAC,IAAI,CAACA,QAAQ;QACtBI,