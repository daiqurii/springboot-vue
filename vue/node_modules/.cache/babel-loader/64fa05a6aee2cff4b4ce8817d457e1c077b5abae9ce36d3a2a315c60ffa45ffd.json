">\r\n            <el-input\r\n                    v-model=\"search\"\r\n                    placeholder=\"搜索\"\r\n                    style=\"width: 20%; margin-right: 5px\"\r\n                    clearable\r\n            ></el-input>\r\n            <el-button type=\"primary\" @click=\"load\">查询</el-button>\r\n        </div>\r\n        <!-- 表格区域 -->\r\n        <el-table :data=\"tableData\" border stripe style=\"width: 100%\">\r\n            <el-table-column prop=\"id\" label=\"ID\"/>\r\n            <el-table-column prop=\"username\" label=\"用户名\"/>\r\n            <el-table-column prop=\"nickname\" label=\"昵称\"/>\r\n            <el-table-column prop=\"age\" label=\"年龄\"/>\r\n            <el-table-column prop=\"sex\" label=\"性别\"/>\r\n            <el-table-column prop=\"address\" label=\"地址\"/>\r\n            <el-table-column fixed=\"right\" label=\"Operations\">\r\n                <template #default=\"scope\">\r\n                    <el-button type=\"primary\" size=\"mini\" @click=\"handleEdit(scope.row)\"\r\n                    >编辑\r\n                    </el-button\r\n                    >\r\n                    <el-popconfirm\r\n                            width=\"200px\"\r\n                            confirm-button-text=\"是的\"\r\n                            cancel-button-text=\"不，谢谢\"\r\n                            :icon=\"InfoFilled\"\r\n                            icon-color=\"#626AEF\"\r\n                            title=\"你确定要删除吗?\"\r\n                    >\r\n                        <template #reference>\r\n                            <el-button type=\"primary\" size=\"mini\" @click=\"handleClick(scope.row)\" style=\"background-color:red\">删除\r\n                            </el-button>\r\n                        </template>\r\n                    </el-popconfirm>\r\n                </template>\r\n            </el-table-column>\r\n        </el-table>\r\n        <!-- 分页 -->\r\n        <div class=\"example-pagination-block\" style=\"margin-top: 5px;\">\r\n            <el-pagination\r\n                    v-model:current-page=\"currentPage\"\r\n                    v-model:page-size=\"pageSize\"\r\n                    :page-sizes=\"[5, 10, 20]\"\r\n                    :small=\"small\"\r\n                    :disabled=\"disabled\"\r\n                    :background=\"background\"\r\n                    layout=\"total, sizes, prev, pager, next, jumper\"\r\n                    :total=\"total\"\r\n                    @size-change=\"handleSizeChange\"\r\n                    @current-change=\"handleCurrentChange\"\r\n            />\r\n        </div>\r\n        <!-- 添加用户信息-->\r\n        <el-dialog\r\n                v-model=\"dialogVisible\"\r\n                title=\"提示\"\r\n                width=\"30%\"\r\n        >\r\n            <!--        添加用户信息表单-->\r\n            <el-form :inline=\"true\" :model=\"form\" label-width=\"120px\">\r\n                <el-form-item label=\"用户名\">\r\n                    <el-input v-model=\"form.username\" placeholder=\"用户名\"/>\r\n                </el-form-item>\r\n                <el-form-item label=\"昵称\">\r\n                    <el-input v-model=\"form.nickname\" placeholder=\"昵称\"/>\r\n                </el-form-item>\r\n                <el-form-item label=\"年龄\">\r\n                    <el-input v-model=\"form.age\" placeholder=\"年龄\"/>\r\n                </el-form-item>\r\n                <el-form-item label=\"性别\">\r\n                    <el-radio-group v-model=\"form.sex\" >\r\n                        <el-radio label=\"男\" size=\"large\">男</el-radio>\r\n                        <el-radio label=\"女\" size=\"large\">女</el-radio>\r\n                        <el-radio label=\"未知\" size=\"large\">未知</el-radio>\r\n                    </el-radio-group>\r\n                </el-form-item>\r\n                <el-form-item label=\"地址\">\r\n                    <el-input type=\"textarea\" v-model=\"form.address\" placeholder=\"地址\"/>\r\n                </el-form-item>\r\n            </el-form>\r\n            <template #footer>\r\n              <span class=\"dialog-footer\">\r\n                <el-button @click=\"dialogVisible = false\">返回</el-button>\r\n                <el-button type=\"primary\" @click=\"save\">\r\n                  提交\r\n                </el-button>\r\n              </span>\r\n            </template>\r\n        </el-dialog>\r\n\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import request from \"../utils/request\";\r\n\r\n    export default {\r\n        name: \"Home\",\r\n        components: {},\r\n        data() {\r\n            return {\r\n                url:\"http://localhost:9090\",\r\n                currentPage:1,\r\n                pageSize:10,\r\n                total:0,\r\n                form: {},\r\n                dialogVisible: false,\r\n                search: \"\",\r\n                tableData: [],\r\n            };\r\n        },\r\n        created() {\r\n            this.load()\r\n        },\r\n        methods: {\r\n            load(){\r\n                var paramsObj = {\r\n                    pageNum:this.currentPage,\r\n                    pageSize:this.pageSize,\r\n                    search:this.search,\r\n                }\r\n              request.get(this.url+\"/user/getAll\",{params:paramsObj}).then(res =>{\r\n                  this.tableData = res.obj.records\r\n                  this.total = res.obj.records.length\r\n              })\r\n            },\r\n            add() {\r\n                // 清空表单 才能添加新对象\r\n                this.form = {};\r\n                //    打开弹窗 Dialog并添加信息\r\n                this.dialogVisible = true;\r\n            },\r\n            save() {\r\n                if(this.form.id){\r\n                    request.put(this.url+\"/user/put\",this.form).then(res => {\r\n                        console.log(res)\r\n                        this.load()\r\n                    })\r\n                }else {\r\n                    request.post(this.url+\"/user/add\",this.form).then(res => {\r\n                        console.log(res)\r\n                        this.load()\r\n                    })\r\n                }\r\n\r\n                this.dialogVisible = false;\r\n            },\r\n            handleEdit(row){\r\n                this.form = JSON.parse(JSON.stringify(row)) //深拷贝\r\n                this.dialogVisible = true\r\n            },\r\n            handleClick() {\r\n\r\n            },\r\n            handleSizeChange() {\r\n\r\n            },\r\n            handleCurrentChange() {\r\n\r\n            },\r\n        },\r\n    };\r\n</script>\r\n\r\n<style scoped>\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}