{"ast":null,"code":"import request from \"../utils/request\";\nexport default {\n  name: \"Home\",\n  components: {},\n  data() {\n    return {\n      url: \"http://localhost:9090\",\n      currentPage: 1,\n      pageSize: 10,\n      total: 0,\n      form: {},\n      dialogVisible: false,\n      search: \"\",\n      tableData: []\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    load() {\n      var paramsObj = {\n        pageNum: this.currentPage,\n        pageSize: this.pageSize,\n        search: this.search\n      };\n      request.get(this.url + \"/user/getAll\", {\n        params: paramsObj\n      }).then(res => {\n        this.tableData = res.obj.records;\n        this.total = res.obj.records.length;\n      });\n    },\n    add() {\n      // 清空表单 才能添加新对象\n      this.form = {};\n      //    打开弹窗 Dialog并添加信息\n      this.dialogVisible = true;\n    },\n    save() {\n      if (this.form.id) {\n        request.put(this.url + \"/user/put\", this.form).then(res => {\n          console.log(res);\n          this.load();\n          if (res.code === 200) {\n            this.$message({\n              type: \"success\",\n              message: \"更新成功\"\n            });\n          } else {\n            this.$message({\n              type: \"error\",\n              message: \"更新失败\"\n            });\n          }\n        });\n      } else {\n        request.post(this.url + \"/user/add\", this.form).then(res => {\n          console.log(res);\n          this.load();\n          if (res.code === 200) {\n            this.$message({\n              type: \"success\",\n              message: \"新增成功\"\n            });\n          } else {\n            this.$message({\n              type: \"error\",\n              message: \"新增失败\"\n            });\n          }\n        });\n      }\n      this.dialogVisible = false;\n    },\n    handleEdit(row) {\n      this.form = JSON.parse(JSON.stringify(row)); //深拷贝\n      this.dialogVisible = true;\n    },\n    handleClick(row) {\n      var id = row.id;\n      request.delete(this.url + \"/user/delete/\" + id).then(res => {\n        this.load();\n        if (res.code === 200) {\n          this.$message({\n            type: \"success\",\n            message: \"删除成功\"\n          });\n        } else {\n          this.$message({\n            type: \"error\",\n            message: \"删除失败\"\n          });\n        }\n      });\n    },\n    handleSizeChange(pageSize) {\n      this.pageSize = pageSize;\n      this.load();\n    },\n    handleCurrentChange(pageNum) {\n      this.currentPage = pageNum;\n      this.load();\n    }\n  }\n};","map":{"version":3,"mappings":"AA2GI,OAAOA,OAAM,MAAO,kBAAkB;AACtC,eAAe;EACXC,IAAI,EAAE,MAAM;EACZC,UAAU,EAAE,CAAC,CAAC;EACdC,IAAI,GAAG;IACH,OAAO;MACHC,GAAG,EAAC,uBAAuB;MAC3BC,WAAW,EAAC,CAAC;MACbC,QAAQ,EAAC,EAAE;MACXC,KAAK,EAAC,CAAC;MACPC,IAAI,EAAE,CAAC,CAAC;MACRC,aAAa,EAAE,KAAK;MACpBC,MAAM,EAAE,EAAE;MACVC,SAAS,EAAE;IACf,CAAC;EACL,CAAC;EACDC,OAAO,GAAG;IACN,IAAI,CAACC,IAAI,EAAC;EACd,CAAC;EACDC,OAAO,EAAE;IACLD,IAAI,GAAE;MACF,IAAIE,SAAQ,GAAI;QACZC,OAAO,EAAC,IAAI,CAACX,WAAW;QACxBC,QAAQ,EAAC,IAAI,CAACA,QAAQ;QACtBI,MAAM,EAAC,IAAI,CAACA;MAChB;MACFV,OAAO,CAACiB,GAAG,CAAC,IAAI,CAACb,GAAG,GAAC,cAAc,EAAC;QAACc,MAAM,EAACH;MAAS,CAAC,CAAC,CAACI,IAAI,CAACC,GAAE,IAAI;QAC/D,IAAI,CAACT,SAAQ,GAAIS,GAAG,CAACC,GAAG,CAACC,OAAM;QAC/B,IAAI,CAACf,KAAI,GAAIa,GAAG,CAACC,GAAG,CAACC,OAAO,CAACC,MAAK;MACtC,CAAC;IACH,CAAC;IACDC,GAAG,GAAG;MACF;MACA,IAAI,CAAChB,IAAG,GAAI,CAAC,CAAC;MACd;MACA,IAAI,CAACC,aAAY,GAAI,IAAI;IAC7B,CAAC;IACDgB,IAAI,GAAG;MACH,IAAG,IAAI,CAACjB,IAAI,CAACkB,EAAE,EAAC;QACZ1B,OAAO,CAAC2B,GAAG,CAAC,IAAI,CAACvB,GAAG,GAAC,WAAW,EAAC,IAAI,CAACI,IAAI,CAAC,CAACW,IAAI,CAACC,GAAE,IAAK;UACpDQ,OAAO,CAACC,GAAG,CAACT,GAAG;UACf,IAAI,CAACP,IAAI,EAAC;UACV,IAAGO,GAAG,CAACU,IAAG,KAAM,GAAG,EAAC;YAChB,IAAI,CAACC,QAAQ,CAAC;cACVC,IAAI,EAAC,SAAS;cACdC,OAAO,EAAC;YACZ,CAAC;UACL,CAAC,MAAI;YACD,IAAI,CAACF,QAAQ,CAAC;cACVC,IAAI,EAAC,OAAO;cACZC,OAAO,EAAC;YACZ,CAAC;UACL;QAEJ,CAAC;MACL,CAAC,MAAK;QACFjC,OAAO,CAACkC